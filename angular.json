{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ex00-new": {
      "root": "projects/ex00-new/",
      "sourceRoot": "projects/ex00-new/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ex00-new",
            "index": "projects/ex00-new/src/index.html",
            "main": "projects/ex00-new/src/main.ts",
            "polyfills": "projects/ex00-new/src/polyfills.ts",
            "tsConfig": "projects/ex00-new/tsconfig.app.json",
            "assets": [
              "projects/ex00-new/src/favicon.ico",
              "projects/ex00-new/src/assets"
            ],
            "styles": ["projects/ex00-new/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ex00-new/src/environments/environment.ts",
                  "with": "projects/ex00-new/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ex00-new:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ex00-new:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ex00-new:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ex00-new/src/test.ts",
            "polyfills": "projects/ex00-new/src/polyfills.ts",
            "tsConfig": "projects/ex00-new/tsconfig.spec.json",
            "karmaConfig": "projects/ex00-new/karma.conf.js",
            "styles": ["projects/ex00-new/src/styles.css"],
            "scripts": [],
            "assets": [
              "projects/ex00-new/src/favicon.ico",
              "projects/ex00-new/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ex00-new/tsconfig.app.json",
              "projects/ex00-new/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
